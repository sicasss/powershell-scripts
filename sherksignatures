Write-Host ForegroundColor Green " Sherk va a comprobar archivos sopechos, ten cuidado :v "
Write-Host ""

# Ruta del archivo strings.txt
$basePath = Get-Location
$stringsFile = Join-Path $basePath "strings.txt"

# Verifica si existe el archivo
if (-Not (Test-Path $stringsFile)) {
    Write-Error "No se encontr√≥ el archivo strings.txt en el directorio actual."
    exit
}

# Lee los paths de .exe desde el archivo
$exePaths = Get-Content $stringsFile

# Imprime encabezado de la tabla
$format = "{0,-60} {1,-20} {2,-30}"
Write-Host ($format -f "Path", "Firma Digital", "USN Journal") -ForegroundColor Yellow
Write-Host ("-" * 110)

foreach ($exePath in $exePaths) {
    $firma = ""
    $usn = ""

    if (Test-Path $exePath) {
        # Verifica firma digital
        try {
            $signature = Get-AuthenticodeSignature $exePath
            switch ($signature.Status) {
                'Valid'      { $firma = "‚úî V√°lida" }
                'NotSigned'  { $firma = "‚ö† No firmada" }
                default      { $firma = "‚ùå Inv√°lida" }
            }
        } catch {
            $firma = "‚ùå Error firma"
        }
        $usn = "‚úî Archivo presente"
    } else {
        $firma = "‚ùå No encontrado"

        # Extrae la unidad del path
        $drive = ($exePath -split ':')[0] + ":"

        # Usa fsutil para consultar el USN Journal (requiere privilegios elevados)
        try {
            $usnOutput = fsutil usn readdata $drive | Out-String
            if ($usnOutput -match [regex]::Escape($exePath)) {
                $usn = "üîç Entrada encontrada"
            } else {
                $usn = "‚ùå No hay rastro"
            }
        } catch {
            $usn = "‚ùå Error USN"
        }
    }

    # Imprime fila de la tabla
    Write-Host ($format -f $exePath, $firma, $usn)
}
